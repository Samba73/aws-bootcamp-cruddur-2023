#!/usr/bin/bash
set -e
CYAN='\033[1;36m'
NO_COLOR='\033[0m'
LABEL="RDS setup"
printf "${CYAN}== ${LABEL}${NO_COLOR}\n"


bin_path="$(realpath .)/bin"

source "$bin_path/db/drop" $1
source "$bin_path/db/create" $1
source "$bin_path/db/scheme-load" $1
source "$bin_path/db/seed" $1
"$bin_path/db/update_cognito_id_db"
